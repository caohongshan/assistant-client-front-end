.container{position:relative}.tabs{position:-webkit-sticky;position:sticky;z-index:2;top:0;background:#fff;padding-bottom:6rpx}.goods-list-container{background:#fff}.goods-list-container.style-padding{background:none}.goods-list-container.style-gride{background:none}.video{height:400rpx;overflow:hidden;background:#999;position:relative}.video .cover{position:absolute;left:0;right:0;top:0;bottom:0;background:rgba(0,0,0,.2901960784313726)}.video .video-image{height:400rpx;width:668rpx;position:absolute;border-radius:12rpx}.video .video-image-icon{position:absolute;top:30%;left:40%;width:120rpx;height:120rpx}.goods-list{margin-top:12rpx;padding:10rpx 26rpx 10rpx 16rpx}.goods-list .goods{background:#fff;border-radius:12rpx;padding:20rpx;display:flex;position:relative;margin-bottom:20rpx;align-items:center}.goods-list .has_description{align-items:inherit}.goods-list .miaosha-cover{position:absolute;left:0;top:0;width:100rpx;z-index:2}.goods-list .goods-image{display:inline-block;width:90rpx;height:90rpx;border-radius:12rpx}.goods-list .goods-content{flex:1;margin-left:24rpx;display:flex;flex-direction:column;justify-content:space-between}.goods-list .desc{color:#999;font-size:24rpx}.goods-list .title{font-size:28rpx}.goods-list.style-padding{background:none;margin:16rpx;padding:0}.goods-list.style-padding .goods{background:#fff;margin-bottom:16rpx;padding:16rpx;border-radius:6rpx}.goods-list.style-gride{display:flex;flex-wrap:wrap;padding:0 16rpx;margin-top:16rpx}.goods-list.style-gride .goods{background:#fff;width:calc(50% - 14rpx);padding:0 0 16rpx;display:block;overflow:hidden;border-radius:12rpx;margin-bottom:16rpx}.goods-list.style-gride .goods .goods-content{margin:0 16rpx}.goods-list.style-gride .goods .goods-image{width:100%;height:350rpx}.goods-list.style-gride .goods:nth-child(2n){margin-left:24rpx}