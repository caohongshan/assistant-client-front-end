page{height:100%}.cart-goods-list-container{height:100%}.cart-goods-list-container .cart-options{position:fixed;bottom:0;left:0;right:0;background:#fff;display:flex;align-items:center;padding:16rpx}.cart-goods-list{padding:10rpx 26rpx 10rpx 16rpx;padding-bottom:calc(0px + 120rpx)}.cart-goods-list .goods{margin-top:14rpx;background:#fff;padding:20rpx 12rpx 14rpx;display:flex;align-items:center;border-radius:6rpx}.cart-goods-list .goods-image{display:inline-block;width:138rpx;height:138rpx;border-radius:12rpx;margin-left:22rpx}.cart-goods-list .goods-content{flex:1;margin-left:24rpx;display:flex;flex-direction:column;justify-content:space-between}.cart-goods-list .title{font-size:28rpx}.cart-goods-list .sub-title{font-size:20rpx;padding:6rpx 12rpx;border-radius:6rpx;margin-top:12rpx;background:#f5f5f5;display:inline-block}